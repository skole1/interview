 <main id="main" class="main">
    <div class="pagetitle">
      <h1>Upload - Page</h1>
      <nav>
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="index.html">Uploads</a></li>
          <li class="breadcrumb-item">Pages</li>
        </ol>
      </nav>
    </div><!-- End Page Title -->

    <section class="section">
      <div class="row">
        <div class="col-lg-12">
          <section class="section dashboard">
              <div class="card card-body">
                <h3 class="text-center mt-3">Upload Excel File</h3>
                <form action="/import" enctype="multipart/form-data" method="post">
                    <input
                    type="file"
                    name="fileupload"
                    accept="application/vnd.ms-excel"
                    class="form-control mt-4"
                    />
                    <div class="text-center">
                      <button type="submit" class="btn btn-dark mt-3" value="Store File">Send</button>
                    </div>
                </form>
              </div>
          </section>
        </div>
      </div>
    </section>

  </main><!-- End #main -->


